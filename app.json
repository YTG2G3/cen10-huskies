{
    "expo": {
        "name": "Cen10 Huskies",
        "slug": "cen10-huskies",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/logo.jpg",
        "userInterfaceStyle": "light",
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true,
            "icon": "./assets/logo.jpg"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/logo_tp.jpg",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./assets/logo_tp.jpg"
        },
        "packagerOpts": {
            "config": "metro.config.js",
            "sourceExts": [
                "js",
                "jsx",
                "scss",
                "sass"
            ]
        },
        "plugins": [
            "expo-localization",
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to let you share them with your community."
                }
            ]
        ]
    }
}